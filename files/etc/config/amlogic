config amlogic 'config'
	option enable '1'
	option amlogic_firmware_repo 'https://github.com/v2clash/ImmortalWrt-N1'
	option amlogic_firmware_tag 'ImmortalWrt'
	option amlogic_firmware_suffix '.img.gz'
	option amlogic_kernel_path 'https://github.com/v2clash/build-armbian'
	option amlogic_kernel_branch '6.6'
	option amlogic_firmware_config '1'
	option amlogic_write_bootloader '1'
	option amlogic_shared_fstype 'ext4'

config settings 'armcpu'
	option governor0 'schedutil'
	option minfreq0 '100000'
	option maxfreq0 '1512000'
